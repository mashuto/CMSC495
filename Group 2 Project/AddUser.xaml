<Page x:Class="Group_2_Project.AddUser"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
	Title="AddUser">

    <Grid>
        <TextBox x:Name="UserNameBox" HorizontalAlignment="Left" Height="23" Margin="86,51,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Password1Box" HorizontalAlignment="Left" Height="23" Margin="86,107,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Password2Box" HorizontalAlignment="Left" Height="23" Margin="86,165,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="AddUserButton" Content="Add User" HorizontalAlignment="Left" Margin="86,220,0,0" VerticalAlignment="Top" Width="120" Click="AddUser_Clicked"/>

    </Grid>
</Page>
